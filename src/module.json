{
  "id": "tapedelay",
  "name": "TapeDelay",
  "abbrev": "TD",
  "version": "0.3.6",
  "description": "Tape delay with flutter and tone shaping",
  "author": "cyrusasfa (port: charlesvestal)",
  "license": "MIT",
  "dsp": "tapedelay.so",
  "api_version": 2,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "TapeDelay",
          "params": [
            {
              "key": "time",
              "label": "Time",
              "type": "int",
              "min": 20,
              "max": 2000,
              "default": 400,
              "step": 10,
              "unit": "ms"
            },
            {
              "key": "feedback",
              "label": "Feedback",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.4,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "mix",
              "label": "Mix",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.35,
              "step": 0.01,
              "unit": "%"
            },
            {
              "key": "tone",
              "label": "Tone",
              "type": "float",
              "min": 0.0,
              "max": 1.0,
              "default": 0.6,
              "step": 0.01,
              "unit": "%"
            }
          ],
          "knobs": ["time", "feedback", "mix", "tone"]
        }
      }
    },
    "chain_params": [
      {
        "key": "time",
        "name": "Time",
        "type": "int",
        "min": 20,
        "max": 2000,
        "default": 400,
        "step": 10
      },
      {
        "key": "feedback",
        "name": "Feedback",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.4,
        "step": 0.01
      },
      {
        "key": "mix",
        "name": "Mix",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.35,
        "step": 0.01
      },
      {
        "key": "tone",
        "name": "Tone",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.6,
        "step": 0.01
      }
    ]
  }
}
