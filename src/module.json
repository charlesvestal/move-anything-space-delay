{
  "id": "tapedelay",
  "name": "TapeDelay",
  "abbrev": "TD",
  "version": "0.3.2",
  "description": "Tape delay with flutter, tone shaping, and saturation",
  "author": "Charles Vestal",
  "license": "MIT",
  "dsp": "tapedelay.so",
  "api_version": 2,
  "capabilities": {
    "chainable": true,
    "component_type": "audio_fx",
    "ui_hierarchy": {
      "levels": {
        "root": {
          "name": "TapeDelay",
          "params": ["time", "feedback", "mix", "tone", "saturation"],
          "knobs": ["time", "feedback", "mix", "tone", "saturation"]
        }
      }
    },
    "chain_params": [
      {
        "key": "time",
        "name": "Time",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.4,
        "step": 0.05
      },
      {
        "key": "feedback",
        "name": "Feedback",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.4,
        "step": 0.05
      },
      {
        "key": "mix",
        "name": "Mix",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.35,
        "step": 0.05
      },
      {
        "key": "tone",
        "name": "Tone",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.6,
        "step": 0.05
      },
      {
        "key": "saturation",
        "name": "Saturation",
        "type": "float",
        "min": 0.0,
        "max": 1.0,
        "default": 0.0,
        "step": 0.05
      }
    ]
  }
}
